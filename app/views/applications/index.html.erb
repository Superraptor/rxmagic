<p id="notice"><%= notice %></p>

<h1>Listing Applications</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Patient ID</th>
      <th>Pharmaceutical Manufacturer ID</th>
      <th>Medication RxNorm NDC</th>
      <th>App Status</th>
      <th>Date Initiated</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @applications.each do |application| %>
      <tr>
        <td><%= application.patientsptid %></td>
        <td><%= application.pharmmanufacturersphid %></td>
        <td><%= application.medicationsrxnormndc %></td>
        <td><%= application.appstatus %></td>
        <td><%= application.dateinit %></td>
        <td><%= link_to 'Show', application %></td>
        <td><%= link_to 'Edit', edit_application_path(application) %></td>
        <td><%= link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application', new_application_path %>
